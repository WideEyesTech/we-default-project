{
  "name": "default-project",
  "version": "1.0.0",
  "description": "default blueprint to kickstart FE projects",
  "repository": "git@github.com:WideEyesTech/we-default-project.git",
  "main": "src/index.js",
  "scripts": {
    "test": "ava | tap-spec",
    "test:watch": "ava --watch",
    "start": "webpack-dev-server --config configs/webpack.dev.js --inline --progress --port 3000",
    "build": "rimraf dist && webpack --config configs/webpack.production.js --progress --profile --bail --release"
  },
  "author": "Gerard Clos",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "ava": "^0.15.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "nock": "^8.0.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "tap-dot": "^1.0.5",
    "sinon": "^1.17.4",
    "redux-mock-store": "^1.1.2",
    "tap-spec": "^4.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^3.2.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.5.2",
    "redux-auth-wrapper": "^0.5.2",
    "redux-logger": "^2.6.1",
    "whatwg-fetch": "^1.0.0",
    "wideeyes-ui": "^1.0.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "require": [
      "babel-register",
      "./configs/testSetup.js"
    ],
    "files": [
      "src/components/**/test/*.js",
      "src/reducers/test/*.js",
      "src/actions/test/*.js"
    ],
    "babel": "inherit",
    "concurrency": 5,
    "failFast": true,
    "tap": true
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  }
}
